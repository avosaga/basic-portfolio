        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-jest": "26.5.3",
        "eslint-plugin-n": "15.0.0",
        "eslint-plugin-promise": "6.0.0",
        "jest": "28.1.1"
    },
    "engines": {
        "node": "16.x.x"
    },
    "jest": {
        "verbose": true,
        "transform": {
            "^.+\\.jsx?$": ["esbuild-jest"]
        }
    }
